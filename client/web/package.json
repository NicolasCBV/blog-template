    "jest": "^29.6.2",
    "jest-environment-jsdom": "^29.6.4",
    "postcss": "^8.4.14",
    "prettier": "^3.0.3",
    "reflect-metadata": "^0.1.13",
    "supertest": "^6.3.3",
    "tailwindcss": "^3.1.8",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typescript": "4.7.4",
    "vercel": "^32.2.0"
