    "@tiptap/extension-code-block-lowlight": "^2.0.4",
    "@tiptap/html": "^2.1.0",
    "@tiptap/pm": "^2.0.4",
    "@tiptap/react": "^2.0.4",
    "@tiptap/starter-kit": "^2.0.4",
    "algoliasearch": "^4.19.1",
    "axios": "^1.6.0",
    "cookie": "^0.5.0",
    "dotenv-cli": "^6.0.0",
    "firebase": "^10.1.0",
    "formidable": "^3.5.1",
    "inversify": "^6.0.1",
    "jsdom": "^20.0.1",
    "lowlight": "^2.9.0",
