{
  "name": "Blog Client Template",
  "version": "0.1.0",
  "private": true,
  "license": "MIT",
  "scripts": {
    "dev": "dotenv -e .env.dev -- next dev",
    "build": "next build",
    "start": "dotenv -e .env.production -- next start",
    "format": "prettier --write \"src/**/*.ts\" \"tests/**/*.ts\"",
    "lint:fix": "eslint \"{src,tests}/**/*.ts\" --fix",
    "test": "dotenv -e .env.test -- jest",
    "test:components": "dotenv -e .env.test -- jest --config ./jest.comp.config.ts",
    "test:watch": "dotenv -e .env.test -- jest --watch",
    "test:cov": "dotenv -e .env.test -- jest --coverage",
    "test:e2e": "cypress open"
  },
  "dependencies": {
    "@radix-ui/react-dropdown-menu": "^2.0.5",
